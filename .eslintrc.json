{
    "extends": "airbnb/base",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        // "no-const-assign": "warn",
        // "no-this-before-super": "warn",
        // "no-undef": "warn",
        // "no-unreachable": "warn",
        // "no-unused-vars": "warn",
        // "constructor-super": "warn",
        // "valid-typeof": "warn",
        "import/no-extraneous-dependencies": 0,
        "linebreak-style": ["error", "windows"],
        "import/first": 0,
        "indent": [2, 4],
        "func-names": 0,
        "no-console": 0,
        "eol-last": 0
    },
    "globals": {
        "import": true
    }
}